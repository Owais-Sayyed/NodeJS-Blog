<div class="author">
  <h1 class="author_heading">Hi, I am ready</h1>
  <p class="author_body">Web Developer</p>
</div>
<img src="/img/hero-image.webp" alt="" width="981" height="528" />

<section class="article">
  <h2 class="article_heading">Latest Posts</h2>
  <ul class="article-ul">
    <% data.forEach(post => { %>
    <li>
      <a href="/post/<%= post._id %>">
        <span><%= post.title %></span>
        <span class="article-list_date"
          ><%= post.createdAt.toDateString() %></span
        >
      </a>
    </li>
    <% }) %>
  </ul>
  <a href="#" class="pagination">&lt; View Older Posts</a>
</section>
